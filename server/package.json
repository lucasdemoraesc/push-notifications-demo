{
  "name": "push-server",
  "version": "1.0.0",
  "main": "./index.js",
  "license": "MIT",
  "scripts": {
    "start": "nodemon --esm -e ts,js ./src/index.ts",
    "build": "rimraf dist && tsc",
    "ts:check": "tsc --project tsconfig.json",
    "add-build": "git add dist",
    "debug": "node --inspect -r ts-node/register ./src/index.ts"
  },
  "pre-commit": [
    "ts:check",
    "build",
    "add-build"
  ],
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^7.3.1",
    "pg-promise": "^11.5.0",
    "typescript": "^5.1.3",
    "web-push": "^3.6.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/mongoose": "^5.11.97",
    "@types/node": "^20.3.2",
    "@types/web-push": "^3.3.2",
    "dotenv": "^16.3.1",
    "nodemon": "^2.0.22",
    "pre-commit": "^1.2.2",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1"
  }
}
